#1
import numpy as np

n = 7
s1 = np.float32(0)
for i in range(1,10**n+1):
    s1 = s1 + np.float32(1/i)

print("suma = ", s1)

s2 = np.float32(0)
for i in range(2*10**n+1,0,-1):
    s2 += np.float32(1/i)

print("suma = ", s2)

